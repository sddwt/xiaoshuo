
.chaptersBox{
	width: 100vw;
	height: 100vh;
	position: fixed;
	padding-top: 40rpx;
	top: 0;
	left: 0;
	z-index: 999;
	overflow-y: auto;
	background: #fff;
}
@font-face {font-family: "iconfont";
  src: url(data:font/ttf;base64,) format('truetype');
}
.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-80dianliang:before {
  content: "\E617";
}
.pagbg{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
.zuizhong.active{
	opacity: 1;
	position: fixed;
	width: 100rpx;
	text-align: center;
	font-size: 30rpx;
	font-weight: bold;
	color: #ec706b;
	-webkit-transform: translateX(50%) scale(0.7);
	        transform: translateX(50%) scale(0.7);
	right: 80rpx;
	-webkit-transition: all 0.5s;
	z-index: 9
}
.zuizhong{
	opacity: 0;
	position: fixed;
	width: 100rpx;
	text-align: center;
	font-size: 20rpx;
	font-weight: bold;
	color: #ec706b;
	right: 80rpx;
	-webkit-transform: translateX(50%)  scale(0.1);
	        transform: translateX(50%)  scale(0.1);
	-webkit-transition: all 0.5s;
	transition: all 0.5s;
	z-index: 9
}
.quanquan{
	box-shadow: rgba(0,0,0,0.12) 0px 3px 13px 1px;
	position: fixed;
	right: 70rpx;
	z-index: 9;
	border-radius: 50%;
	overflow: hidden;
	-webkit-transform: translateX(50%) scale(0.5);
	        transform: translateX(50%) scale(0.5);
}
.back{
	width: 40rpx;
	height: 40rpx;
	margin: 0 40rpx;
}
.sview{
	width: calc(100% - 40rpx);
	font-size: 45rpx;
	line-height: 90rpx;
	position: relative;
	text-indent:calc(2em + 12rpx);
	margin: 0 auto;
	z-index: 5;
	white-space:normal;
	word-break:break-all;
	word-wrap:break-word;
	overflow: hidden;
	padding: 0 20rpx 30rpx;
}
.titlee{
	width: 100%;
	font-size: 45rpx;
	line-height: 65rpx;
	position: relative;
	z-index: 5;
	padding: 0 20rpx 50rpx;
	text-indent:-2.3em;
}
.tMain{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align:center;
	-webkit-align-items:center;
	        align-items:center;
}
page{
	letter-spacing:6rpx;
}
.topBox{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	opacity: 1;
	z-index: 9;
}
.bottomBox{
	padding: 0 0 20rpx 0;
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	opacity: 1;
	z-index: 9;
}
.bottomBox .ddd>view{
	width: 100%;
	text-align: center;
	font-size: 24rpx;
	line-height: 40rpx;
}
.bottomBox .ddd image{
	width: 40rpx;
	height: 40rpx;
}
@font-face {font-family: "ydiconfont";
  src: url('https://at.alicdn.com/t/font_1282539_9h0uwv1sxps.ttf') format('truetype'); /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
}
.tficon{
	font-family: ydiconfont;
	font-size: 34rpx;
}
.guanggao{
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 6;
	width: 100%;
}
.dianxin{
	position: absolute;
	top: 32.5%;
	left: 11%;
	width: 72%;
	height: 35%;
}
.dLiang{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	display: block;
}
.anmt{
	-webkit-transition: all 0.5s;
	transition: all 0.5s;
}
.sekuai{
	width: 150rpx;
	height: 100rpx;
	background-color: #EC706B;
	border-radius: 12rpx;
	border: 5rpx solid #000;
	-webkit-transform: scale(0.4);
	        transform: scale(0.4);
	margin: -10rpx -35rpx 0;
}
.zhongMenu{
	top: 50%;
	position: fixed;
	left: 0;
	width: 100%;
	height: 50%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	z-index: 9;
}

